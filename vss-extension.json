{
  "manifestVersion": 1,
  "id": "",
  "version": "",
  "name": "",
  "publisher": "",
  "scopes": [
    "vso.work",
    "vso.dashboards_manage"
  ],
  "description": "Backlog Item Hierarchy",
  "galleryFlags": [
    "Preview"
  ],
  "icons": {
    "default": "static/images/logo.png"
  },
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services.Cloud"
    }
  ],
  "demands": [
    "api-version/3.0"
  ],
  "tags": [
    "Work Item","Board"
  ],
  "content": {
    "license": {
      "path": "license.md"
    },
    "details": {
      "path": "overview.md"
    }
  },
  "links": {
    "getstarted": {
      "uri": "https://github.com/Cellenza/Azure-DevOps-Extension-WI-Hierarchy-Widget/blob/master/README.md"
    },
    "support": {
      "uri": "https://github.com/Cellenza/Azure-DevOps-Extension-WI-Hierarchy-Widget/issues"
    },
    "issues": {
      "uri": "https://github.com/Cellenza/Azure-DevOps-Extension-WI-Hierarchy-Widget/issues"
    }
  },
  "repository": {
    "type": "git",
    "uri": "https://github.com/Cellenza/Azure-DevOps-Extension-WI-Hierarchy-Widget"
  },
  "branding": {
    "color": "rgb(220, 235, 252)",
    "theme": "light"
  },
  "files": [
    {
      "path": "dist",
      "addressable": true
    },
    {
      "path": "static",
      "addressable": true
    },
    {
      "path": "node_modules/vss-web-extension-sdk/lib",
      "addressable": true,
      "packagePath": "lib"
    },
    {
      "path": "node_modules/jquery/dist",
      "addressable": true,
      "packagePath": "lib"
    },
    {
      "path": "lib",
      "addressable": true,
      "packagePath": "lib"
    }
  ],
  "categories": [
    "Azure Boards"
  ],
  "contributions": [
    {
      "id": "addToDashboard-backlogitemhierarchy-menu",
      "type": "ms.vss-web.action-provider",
      "description": "",
      "targets": [
        "ms.vss-work-web.work-item-context-menu"
      ],
      "properties": {
        "group": "contributed",
        "uri": "static/addtodashboard.html"
      }
    },
    {
      "id": "backlogitemhierarchy",
      "type": "ms.vss-dashboards-web.widget",
      "targets": [
        "ms.vss-dashboards-web.widget-catalog",
        ".backlogitemhierarchy-Configuration"
      ],
      "properties": {
        "name": "Backlog Item Hierarchy",
        "description": "backlog item hierarchy",
        "uri": "static/index.html",
        "isNameConfigurable": true,
        "catalogIconUrl:": "static/images/logo.png",
        "previewImageUrl": "static/images/logo.png",
        "supportedSizes": [
          {
            "rowSpan": 1,
            "columnSpan": 3
          },
          {
            "rowSpan": 2,
            "columnSpan": 3
          },
          {
            "rowSpan": 3,
            "columnSpan": 3
          },
          {
            "rowSpan": 4,
            "columnSpan": 3
          },
          {
            "rowSpan": 5,
            "columnSpan": 3
          }
        ],
        "supportedScopes": [
          "project_team"
        ]
      }
    },
    {
      "id": "backlogitemhierarchy-Configuration",
      "type": "ms.vss-dashboards-web.widget-configuration",
      "targets": [
        "ms.vss-dashboards-web.widget-configuration"
      ],
      "properties": {
        "name": "Backlog Item Hierarchy Configuration",
        "description": "backlog item hierarchy.",
        "uri": "static/configuration.html"
      }
    }
  ]
}